<%- include('../partials/header'); %>
    <%- include('../partials/top-header'); %>
        <%- include('./inc/sidebar'); %>

            <div class="page-content">
                <div class="container-fluid">

                    <!-- start page title -->
                    <div class="row">
                        <div class="col-12">
                            <div class="page-title-box d-sm-flex align-items-center justify-content-between">
                                <h4 class="mb-sm-0">admin</h4>

                                <div class="page-title-right">
                                    <ol class="breadcrumb m-0">
                                        <li class="breadcrumb-item"><a href="javascript: void(0);">account</a></li>
                                        <li class="breadcrumb-item active">members</li>
                                    </ol>
                                </div>

                            </div>
                        </div>
                    </div>
                    <!-- end page title -->

                    <div class="row">
                        <div class="col-lg-12">
                            <div class="card">
                                <div class="card-header">
                                    <h4 class="card-title mb-0 flex-grow-1">members</h4>
                                </div><!-- end card header -->

                                <div class="card-body">
                                    <table id="myTable" class="display">
                                        <thead>
                                            <tr>
                                                <th>Fullname</th>
                                                <th>email</th>
                                                <th>Balance</th>
                                                <th>Deposit</th>
                                                <th>Withdrawing</th>
                                                <th>Trading</th>
                                                <th>Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% if(typeof members !=='undefined' && members !=="" && typeof
                                                members==='object' ){ %>
                                                <% members.forEach((doc)=> { %>
                                                    <tr>
                                                        <td><%- (typeof doc.data().firstname !=='undefined' && typeof doc.data().lastname !=='undefined') ?
                                                                doc.data().firstname + ' ' + doc.data().lastname : '' ; %></td>
                                                        <td><%- (typeof doc.data().email !=='undefined' ) ?
                                                                doc.data().email : '' ; %></td>
                                                        <td><%- (typeof doc.data().balance !=='undefined' ) ?
                                                                doc.data().balance : '' ; %></td>
                                                        <td><%- (typeof doc.data().deposit !=='undefined' ) ? doc.data().deposit : '' ; %></td>
                                                        <td><%- (typeof doc.data().withdrawal !=='undefined' ) ? doc.data().withdrawal : '' ; %></td>
                                                        <td><%- (typeof doc.data().trading!=='undefined' ) ? doc.data().trading : '' ;%></span>
                                                        </td>
                                                        <td>
                                                            <!-- <a href="#" class="btn btn-primary btn-sm">View Profile</a> -->
                                                            <a href="javascript:void(0)" onclick="alert('premium function !!')" class="btn btn-danger btn-sm">Lock</a>
                                                        </td>
                                                    </tr>
                                                    <% }) %>
                                                        <% } else { %>
                                                            <h4>You have no item yet yet !</h4>
                                                            <% } %>

                                        </tbody>
                                    </table>
                                </div><!-- end card-body -->
                            </div><!-- end card -->
                        </div>
                        <!-- end col -->
                    </div>
                    <!-- end row -->



                </div>
                <!-- container-fluid -->
            </div>
            <!-- End Page-content -->
            <link rel="stylesheet" href="https://cdn.datatables.net/1.13.6/css/jquery.dataTables.css" />

            <script src="https://cdn.datatables.net/1.13.6/js/jquery.dataTables.js"></script>
            <%- include('../partials/footer'); %>
                <script>
                    $(document).ready(function () {
                        $('#myTable').DataTable();
                    });
                </script>